<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.panding.main.carclub.fragment.ReserveFragment">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include layout="@layout/toolbar"/>


        <LinearLayout
            android:background="@color/white"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_weight="1397"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:textSize="18sp"
                        android:textColor="@android:color/black"
                        android:layout_marginLeft="24dp"
                        android:gravity="center"
                        android:text="车牌号码"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_weight="2417"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">
                    <EditText
                        android:layout_marginTop="4dp"
                        android:background="@drawable/et_tran_bg"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:background="@color/itembg"
                android:layout_width="match_parent"
                android:layout_height="8dp"></LinearLayout>

            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_weight="1397"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_marginLeft="24dp"
                        android:gravity="center"
                        android:text="预约人姓名"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_weight="2417"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">
                    <EditText
                        android:layout_marginTop="4dp"
                        android:background="@drawable/et_tran_bg"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="24dp"
                android:layout_marginRight="8dp"
                android:background="@drawable/shape_line"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></LinearLayout>

            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_weight="1397"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_marginLeft="24dp"
                        android:gravity="center"
                        android:text="预约人电话"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_weight="2417"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">
                    <EditText
                        android:layout_marginTop="4dp"
                        android:background="@drawable/et_tran_bg"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:background="@color/itembg"
                android:layout_width="match_parent"
                android:layout_height="8dp"></LinearLayout>

            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_weight="1397"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_marginLeft="24dp"
                        android:gravity="center"
                        android:text="预约人数"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_weight="2417"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">
                    <EditText
                        android:layout_marginTop="4dp"
                        android:background="@drawable/et_tran_bg"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="24dp"
                android:layout_marginRight="8dp"
                android:background="@drawable/shape_line"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></LinearLayout>

            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_weight="1397"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_marginLeft="24dp"
                        android:gravity="center"
                        android:text="参与时间"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_weight="2417"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content">
                    <EditText
                        android:layout_marginTop="4dp"
                        android:background="@drawable/et_tran_bg"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:background="@color/itembg"
                android:layout_width="match_parent"
                android:layout_height="8dp"></LinearLayout>

        </LinearLayout>


        <EditText
            android:layout_marginTop="8dp"
            android:inputType="textMultiLine"
            android:textSize="12sp"
            android:gravity="left"
            android:hint="留言"
            android:background="@drawable/et_tran_bg"
            android:layout_marginRight="24dp"
            android:layout_marginLeft="24dp"
            android:layout_width="match_parent"
            android:layout_height="72dp" />

        <Button
            android:layout_marginTop="16dp"
            android:text="提交预约"
            android:layout_marginRight="36dp"
            android:layout_marginLeft="36dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </LinearLayout>


</FrameLayout>
